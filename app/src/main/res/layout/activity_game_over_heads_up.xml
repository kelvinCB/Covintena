<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondojuegos"
    tools:context=".GameOverHeadsUpActivity">

    <TextView
        android:id="@+id/textView74"
        android:layout_width="336dp"
        android:layout_height="673dp"
        android:background="#1F1F1F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092" />

    <TextView
        android:id="@+id/textView89"
        android:layout_width="336dp"
        android:layout_height="245dp"
        android:background="#4B4B4B"
        app:layout_constraintBottom_toBottomOf="@+id/textView74"
        app:layout_constraintEnd_toEndOf="@+id/textView74"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView74"
        app:layout_constraintTop_toBottomOf="@+id/imageView40"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/botones_menu"
        android:gravity="center"
        android:text="Incorrectas"
        android:textColor="@color/blanco"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView89"
        app:layout_constraintEnd_toStartOf="@+id/tvIncorrectas"
        app:layout_constraintHorizontal_bias="0.662"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView62"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView62"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/botones_menu"
        android:gravity="center"
        android:text="Correctas"
        android:textColor="@color/blanco"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tvCorrectas"
        app:layout_constraintHorizontal_bias="0.354"
        app:layout_constraintStart_toStartOf="@+id/textView89"
        app:layout_constraintTop_toTopOf="@+id/textView89" />

    <TextView
        android:id="@+id/tvCorrectas"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/botones_menu"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/blanco"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView89"
        app:layout_constraintTop_toTopOf="@+id/textView89" />

    <TextView
        android:id="@+id/tvIncorrectas"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/botones_menu"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/blanco"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView89"
        app:layout_constraintEnd_toEndOf="@+id/textView74"
        app:layout_constraintTop_toBottomOf="@+id/tvCorrectas"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/volverJugar_hu"
        android:layout_width="270dp"
        android:layout_height="55dp"
        android:background="@color/blanco"
        android:text="VOLVER A JUGAR"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView74"
        app:layout_constraintEnd_toEndOf="@+id/textView89"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="@+id/textView89"
        app:layout_constraintTop_toBottomOf="@+id/textView89"
        app:layout_constraintVertical_bias="0.142" />

    <Button
        android:id="@+id/salir_hu"
        android:layout_width="270dp"
        android:layout_height="55dp"
        android:layout_marginTop="20dp"
        android:background="@color/blanco"
        android:text="SALIR"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView92"
        app:layout_constraintEnd_toEndOf="@+id/textView89"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="@+id/textView89"
        app:layout_constraintTop_toBottomOf="@+id/volverJugar_hu"
        app:layout_constraintVertical_bias="0.257" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="330dp"
        android:layout_height="257dp"
        app:layout_constraintEnd_toEndOf="@+id/textView74"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView74"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gameover2" />

</androidx.constraintlayout.widget.ConstraintLayout>
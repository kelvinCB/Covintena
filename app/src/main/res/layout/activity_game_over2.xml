<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1F1F1F"
    tools:context=".GameOverActivity">


    <TextView
        android:id="@+id/textView96"
        android:layout_width="329dp"
        android:layout_height="263dp"
        android:background="#4B4B4B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView26" />

    <TextView
        android:id="@+id/textViewResultados"
        android:layout_width="215dp"
        android:layout_height="138dp"
        android:background="@drawable/botones_menu"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/blanco"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView96"
        app:layout_constraintEnd_toEndOf="@+id/textView96"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="@+id/textView96"
        app:layout_constraintTop_toBottomOf="@+id/textView93"
        app:layout_constraintVertical_bias="0.24"
        tools:text="RESULTADO" />

    <TextView
        android:id="@+id/textView93"
        android:layout_width="215dp"
        android:layout_height="88dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/botones_menu"
        android:gravity="center"
        android:text="SCORE"
        android:textColor="@color/blanco"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView96"
        app:layout_constraintStart_toStartOf="@+id/textView96"
        app:layout_constraintTop_toTopOf="@+id/textView96"
        tools:text="SCORE" />

    <Button
        android:id="@+id/Salir"
        android:layout_width="283dp"
        android:layout_height="62dp"
        android:layout_marginTop="12dp"
        android:background="@color/blanco"
        android:text="SALIR"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView96"
        app:layout_constraintStart_toStartOf="@+id/textView96"
        app:layout_constraintTop_toBottomOf="@+id/textView96"
        app:layout_constraintVertical_bias="0.347" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="330dp"
        android:layout_height="257dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gameover2" />

</androidx.constraintlayout.widget.ConstraintLayout>
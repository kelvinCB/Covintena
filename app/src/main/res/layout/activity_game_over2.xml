<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondojuegos"
    tools:context=".GameOverActivity">

    <TextView
        android:id="@+id/textView92"
        android:layout_width="336dp"
        android:layout_height="673dp"
        android:background="#1F1F1F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.716" />

    <TextView
        android:id="@+id/textView96"
        android:layout_width="318dp"
        android:layout_height="302dp"
        android:layout_marginTop="164dp"
        android:background="#4B4B4B"
        app:layout_constraintEnd_toEndOf="@+id/textView92"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="@+id/textView92"
        app:layout_constraintTop_toTopOf="@+id/textView92" />

    <TextView
        android:id="@+id/textView93"
        android:layout_width="215dp"
        android:layout_height="88dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/botones_menu"
        android:gravity="center"
        android:text="SCORE"
        android:textColor="@color/blanco"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView96"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@+id/textView96"
        app:layout_constraintTop_toTopOf="@+id/textView96"
        tools:text="SCORE" />

    <TextView
        android:id="@+id/textViewResultados"
        android:layout_width="221dp"
        android:layout_height="147dp"
        android:background="@drawable/botones_menu"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/blanco"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView96"
        app:layout_constraintEnd_toEndOf="@+id/textView96"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/textView96"
        app:layout_constraintTop_toBottomOf="@+id/textView93"
        app:layout_constraintVertical_bias="0.259"
        tools:text="RESULTADO" />


    <Button
        android:id="@+id/Salir"
        android:layout_width="270dp"
        android:layout_height="55dp"
        android:background="@color/blanco"
        android:text="SALIR"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView92"
        app:layout_constraintEnd_toEndOf="@+id/textView92"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="@+id/textView92"
        app:layout_constraintTop_toBottomOf="@+id/textView96"
        app:layout_constraintVertical_bias="0.302" />

    <Button
        android:id="@+id/VolverJugar"
        android:layout_width="270dp"
        android:layout_height="55dp"
        android:background="@color/blanco"
        android:text="Volver a Jugar"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView92"
        app:layout_constraintEnd_toEndOf="@+id/textView92"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="@+id/textView92"
        app:layout_constraintTop_toBottomOf="@+id/textView96"
        app:layout_constraintVertical_bias="0.842" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="346dp"
        android:layout_height="349dp"
        app:layout_constraintEnd_toEndOf="@+id/textView92"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView92"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gameover2" />

</androidx.constraintlayout.widget.ConstraintLayout>